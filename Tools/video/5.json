INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+李文博+南平车务段主图.jpg', '李文博', '南平车务段', '如何将废弃竹签加以利用，制造出一件让人瞠目乍舌的工艺品，既新颖美观又环保。倡议环保！',  '“我的绝活”最牛视频展示+李文博+南平车务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+李森+福州车务段主图.jpg', '李森', '福州车务段', '李森擅长在镜头前表演，在今年8月福州车务段拍摄的“福车小剧场”中，将“大哥”的形象演绎的活灵活现，展现了青年多才多艺“最美”的一面。',  '“我的绝活”最牛视频展示+李森+福州车务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+陈阳+鹰潭工务机械段主图.jpg', '陈阳', '鹰潭工务机械段', '这些让人分分钟想捞回家的金鱼竟然是假的，你还相信你的眼镜吗。鱼儿水中游，事实上，鱼儿是在画中游，陈阳通过浇淋树脂凝胶、一笔一画绘出了以假乱真的金鱼。',  '“我的绝活”最牛视频展示+陈阳+鹰潭工务机械段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+郑明聪+福州机务段主图.jpg', '郑明聪', '福州机务段', '笔酣墨饱间如行云流水，落笔如云烟，手臂婉转间笔锋忽然劲健，如壮士拔剑，神采动人，观郑明聪书法如沐烟霞！',  '“我的绝活”最牛视频展示+郑明聪+福州机务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+褚帅+厦门工务段主图.jpg', '褚帅', '厦门工务段', '喜欢B-Box的节奏感的褚帅在工作之余也常给大家露一手，你永远想不到他会发出什么样奇妙的声音，让我们走进他的B-Box音乐之旅。',  '“我的绝活”最牛视频展示+褚帅+厦门工务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+王国强+福州东车辆段主图.jpg', '王国强', '福州东车辆段', '蒙眼组装货车制动管路,把练习技能当成了乐趣。',  '“我的绝活”最牛视频展示+王国强+福州东车辆段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+王运涛+南昌供电段主图.jpg', '王运涛', '南昌供电段', '下锚拉线回头制作，是一个展示力量和体现技术的项目，同时也是一个速度与技艺的完美结合的项目。',  '“我的绝活”最牛视频展示+王运涛+南昌供电段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+宋智天+赣州车务段主图.jpg', '宋智天', '赣州车务段', '爱乐是他的名片，爱逗别人乐是他的特点，来自东北铁岭的宋智天，爱讲相声，爱做饭，一枚扎根在南方的阳光东北小伙。',  '“我的绝活”最牛视频展示+宋智天+赣州车务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+管宜枫+九江车务段主图.jpg', '管宜枫', '九江车务段', '一提起童年，总是让人陷入无限美好之中。视频中的仿真火车模型和南昌火车站mini版都出自一位90后小伙之手，他就是管宜枫，从小就爱动手的他一入职，就开始制作各类火车模型。',  '“我的绝活”最牛视频展示+管宜枫+九江车务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+韩宇坤+南昌车站主图.jpg', '韩宇坤', '南昌车站', '手工达人展现了玲珑骰子的制作方法。',  '“我的绝活”最牛视频展示+韩宇坤+南昌车站主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+傅安泰+南昌机务段主图.jpg', '傅安泰', '南昌机务段', '以机车乘务员一次出乘作业为内容，结合《一人饮酒醉》说唱伴奏，围绕“强基达标、提质增效”主题填写歌词，展示机车乘务员标准化作业风采。制作：胡斌　王智明',  '“我的绝活”最牛视频展示+傅安泰+南昌机务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+黄谦+南昌南车辆段主图.jpg', '黄谦', '南昌南车辆段', '黄谦热爱新潮的运动和武术，尤其喜爱跑酷和双节棍。视频中动作套路刚柔并济、行云流水，展示了一定的武术水准。制作：黄谦',  '“我的绝活”最牛视频展示+黄谦+南昌南车辆段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+张兴玉+南昌通信段主图.jpg', '张兴玉', '南昌通信段', '用最快的速度做最精密的活，而且也要做到精确。',  '“我的绝活”最牛视频展示+张兴玉+南昌通信段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+蒲彪+南平工务段主图.jpg', '蒲彪', '南平工务段', '喜欢用图画表达，更希望用画展现南铁青年的朝气、活力与梦想。',  '“我的绝活”最牛视频展示+蒲彪+南平工务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
INSERT INTO voteitems VALUES (null, 'young', '“我的绝活”', '最牛视频展示', 'video', '“我的绝活”最牛视频展示+胡洋+上饶车务段主图.jpg', '胡洋', '上饶车务段', '胡洋，从小练习跆拳道，14岁参加中国第五届中学生锦标赛（华北赛区）并荣获个人竞技季军；15岁代表山东省跆拳道协会参加全国冠军赛荣获个人击破冠军。',  '“我的绝活”最牛视频展示+胡洋+上饶车务段主图.mp4', null, null, 0, 1, '2017-11-26 19:35:06', '2017-11-26 19:35:06');
